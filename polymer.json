{
  "entrypoint": "index.html",
  "shell": "src/kc-app.html",
  "fragments": [
    "src/pages/kc-page-browse.html",
    "src/pages/kc-page-chapter.html",
    "src/pages/kc-page-not-found.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "chapters/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
